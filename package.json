{
	"name": "sveltekit-vendure-starter",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "concurrently 'vite dev' 'yarn codegen --watch'",
		"build": "shx rm -rf ./build && yarn codegen && vite build",
		"preview": "vite preview",
		"test": "yarn test:integration && yarn test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"codegen": "graphql-codegen"
	},
	"devDependencies": {
		"@googlemaps/js-api-loader": "^1.16.6",
		"@graphql-codegen/cli": "^5.0.2",
		"@graphql-codegen/client-preset": "^4.2.5",
		"@melt-ui/svelte": "^0.76.2",
		"@parcel/watcher": "^2.4.1",
		"@playwright/test": "^1.42.1",
		"@sveltejs/adapter-cloudflare": "^4.2.0",
		"@sveltejs/adapter-node": "^5.0.1",
		"@sveltejs/adapter-vercel": "^5.2.0",
		"@sveltejs/kit": "^2.5.4",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@tailwindcss/forms": "^0.5.6",
		"@tailwindcss/typography": "^0.5.12",
		"@types/braintree-web-drop-in": "^1.39.3",
		"@types/google.maps": "^3.55.5",
		"autoprefixer": "^10.4.19",
		"braintree-web-drop-in": "^1.42.0",
		"concurrently": "^8.2.2",
		"dotenv": "^16.4.5",
		"formsnap": "^1.0.0",
		"graphql": "^16.8.1",
		"lucide-svelte": "^0.363.0",
		"shx": "^0.3.4",
		"svelte": "^4.2.12",
		"svelte-check": "^3.6.8",
		"svelte-meta-tags": "^3.1.1",
		"sveltekit-search-params": "^2.1.2",
		"sveltekit-stripe": "file:../sveltekit-stripe",
		"sveltekit-superfetch": "^3.0.3",
		"sveltekit-superforms": "^2.11.0",
		"sveltekit-turnstile": "^1.2.0",
		"svoast": "^2.4.4",
		"tailwindcss": "^3.4.2",
		"tslib": "^2.4.1",
		"typescript": "^5.4.3",
		"vite": "5.2.6",
		"vite-plugin-cjs-interop": "^2.1.0",
		"vitest": "1.4.0",
		"xss": "^1.0.15",
		"zod": "^3.22.4"
	},
	"dependencies": {
		"@urql/svelte": "^4.1.0"
	},
	"type": "module"
}
